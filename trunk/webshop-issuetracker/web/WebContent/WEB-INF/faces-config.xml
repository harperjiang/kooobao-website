<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
	</application>
	<managed-bean>
		<managed-bean-name>createIssue</managed-bean-name>
		<managed-bean-class>com.kooobao.wsm.web.issue.CreateIssueBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>issueDao</property-name>
			<value>#{issueDao}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>createIssueSuccess</managed-bean-name>
		<managed-bean-class>com.kooobao.wsm.web.issue.CreateIssueSuccessBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>supportData</managed-bean-name>
		<managed-bean-class>com.kooobao.wsm.web.support.SupportDataBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
		<managed-property>
			<property-name>userService</property-name>
			<value>#{userService}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>issueSummary</managed-bean-name>
		<managed-bean-class>com.kooobao.wsm.web.issue.IssueSummaryBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>issueDao</property-name>
			<value>#{issueDao}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>viewIssueList</managed-bean-name>
		<managed-bean-class>com.kooobao.wsm.web.issue.ViewIssueListBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>issueDao</property-name>
			<value>#{issueDao}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>viewIssue</managed-bean-name>
		<managed-bean-class>com.kooobao.wsm.web.issue.ViewIssueBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>issueDao</property-name>
			<value>#{issueDao}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>login</managed-bean-name>
		<managed-bean-class>com.kooobao.authcenter.web.bean.LoginBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>authService</property-name>
			<value>#{authenticateService}</value>
		</managed-property>
	</managed-bean>
	<navigation-rule>
		<display-name>create_issue.xhtml</display-name>
		<from-view-id>/create_issue.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{createIssue.save}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/create_issue_success.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>index.xhtml</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/view_issue_list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>view_issue_list.xhtml</display-name>
		<from-view-id>/view_issue_list.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{viewIssueList.view}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/view_issue.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>view_issue.xhtml</display-name>
		<from-view-id>/view_issue.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{viewIssue.saveTrack}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/view_issue.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>require_login</display-name>
		<navigation-case>
			<from-outcome>require_login</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>logout</display-name>
		<navigation-case>
			<from-action>#{login.logout}</from-action>
			<from-outcome>true</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>create_issue_success.xhtml</display-name>
		<from-view-id>/create_issue_success.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{createIssueSuccess.viewCreated}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/view_issue.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>create_issue_success.xhtml</display-name>
		<from-view-id>/create_issue_success.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{createIssueSuccess.newIssue}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/create_issue.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>view_issue.xhtml</display-name>
		<from-view-id>/view_issue.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{viewIssue.saveIssue}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/view_issue.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<lifecycle>
		<phase-listener>com.kooobao.authcenter.listener.LoginAuthorizer</phase-listener>
		<phase-listener>com.kooobao.common.web.bean.BeanInitializer</phase-listener>
	</lifecycle>

</faces-config>
