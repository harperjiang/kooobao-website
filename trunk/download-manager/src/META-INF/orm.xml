<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings version="2.1"
	xmlns="http://www.eclipse.org/eclipselink/xsds/persistence/orm"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.eclipse.org/eclipselink/xsds/persistence/orm http://www.eclipse.org/eclipselink/xsds/eclipselink_orm_2_1.xsd">

	<table-generator name="fileItemTG" table="dm_sequence"
		pk-column-name="name" value-column-name="count" pk-column-value="dm_file_item" />

	<entity class="com.kooobao.dm.FileItem">
		<primary-key>
			<column name="obj_id" />
		</primary-key>
		<attributes>
			<basic name="oid">
				<column name="obj_id" />
				<generated-value generator="fileItemTG" />
			</basic>
		</attributes>
	</entity>
</entity-mappings>
