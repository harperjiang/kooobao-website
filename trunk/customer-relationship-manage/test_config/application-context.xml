<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<import resource="env.xml" />

	<bean id="hintDao" class="com.kooobao.crm.customer.dao.JpaHintDao">
		<property name="entityManager" ref="entityManager" />
	</bean>

	<bean id="customerDao" class="com.kooobao.crm.customer.dao.JpaCustomerDao">
		<property name="entityManager" ref="entityManager" />
	</bean>

	<bean id="settingDao" class="com.kooobao.crm.setting.dao.JpaSettingDao">
		<property name="entityManager" ref="entityManager" />
	</bean>

</beans>