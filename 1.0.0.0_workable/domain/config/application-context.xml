<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<import resource="env.xml" />

	<!-- bean id="productDao" class="com.kooobao.wsm.domain.dao.jpa.JpaIssueDao"> 
		<property name="entityManager" ref="entityManager"/> </bean -->

	<!-- bean id="orderDao" class="com.kooobao.wsm.domain.dao.jpa.JpaUserDao"> 
		<property name="entityManager" ref="entityManager"/> </bean -->

	<bean id="productDao" class="com.kooobao.gsm.domain.dao.jpa.JpaProductDao">
		<property name="entityManager" ref="entityManager"/>
	</bean>
	<bean id="orderDao" class="com.kooobao.gsm.domain.dao.jpa.JpaOrderDao">
		<property name="entityManager" ref="entityManager"/>
	</bean>
	<bean id="deliveryDao" class="com.kooobao.gsm.domain.dao.jpa.JpaDeliveryDao">
		<property name="entityManager" ref="entityManager"/>
	</bean>
	<bean id="groupDao" class="com.kooobao.gsm.domain.dao.jpa.JpaGroupDao">
		<property name="entityManager" ref="entityManager"/>
	</bean>
	<bean id="supportDao" class="com.kooobao.gsm.domain.dao.xml.XmlSupportDao" />

	<bean id="deliveryService" class="com.kooobao.gsm.service.DeliveryService">
		<property name="deliveryDao" ref="deliveryDao" />
		<property name="orderDao" ref="orderDao" />
	</bean>
</beans>