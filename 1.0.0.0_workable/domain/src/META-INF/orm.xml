<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings version="2.1"
	xmlns="http://www.eclipse.org/eclipselink/xsds/persistence/orm"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.eclipse.org/eclipselink/xsds/persistence/orm http://www.eclipse.org/eclipselink/xsds/eclipselink_orm_2_1.xsd">
	<table-generator name="orderTG" table="gsm_sequence"
		pk-column-name="name" value-column-name="count" pk-column-value="gsm_order" />
	<table-generator name="orderItemTG" table="gsm_sequence"
		pk-column-name="name" value-column-name="count" pk-column-value="gsm_order_item" />
	<table-generator name="doTG" table="gsm_sequence"
		pk-column-name="name" value-column-name="count" pk-column-value="gsm_delivery" />
	<table-generator name="doItemTG" table="gsm_sequence"
		pk-column-name="name" value-column-name="count" pk-column-value="gsm_delivery_item" />
	<table-generator name="productTG" table="gsm_sequence"
		pk-column-name="name" value-column-name="count" pk-column-value="gsm_product" />
	<table-generator name="groupTG" table="gsm_sequence"
		pk-column-name="name" value-column-name="count" pk-column-value="gsm_group" />


	<entity class="com.kooobao.gsm.domain.entity.order.Order">
		<primary-key>
			<column name="obj_id" />
		</primary-key>
		<attributes>
			<basic name="oid">
				<column name="obj_id" column-definition="decimal(10)"/>
				<generated-value generator="orderTG" />
			</basic>
		</attributes>
	</entity>

	<entity class="com.kooobao.gsm.domain.entity.order.OrderItem">
		<primary-key>
			<column name="obj_id" />
		</primary-key>
		<attributes>
			<basic name="oid">
				<column name="obj_id" column-definition="decimal(10)"/>
				<generated-value generator="orderItemTG" />
			</basic>
		</attributes>
	</entity>

	<entity class="com.kooobao.gsm.domain.entity.delivery.Delivery">
		<primary-key>
			<column name="obj_id" />
		</primary-key>
		<attributes>
			<basic name="oid">
				<column name="obj_id" column-definition="decimal(10)"/>
				<generated-value generator="doTG" />
			</basic>
		</attributes>
	</entity>

	<entity class="com.kooobao.gsm.domain.entity.delivery.DeliveryItem">
		<primary-key>
			<column name="obj_id" />
		</primary-key>
		<attributes>
			<basic name="oid">
				<column name="obj_id" column-definition="decimal(10)"/>
				<generated-value generator="doItemTG" />
			</basic>
		</attributes>
	</entity>

	<entity class="com.kooobao.gsm.domain.entity.product.Product">
		<primary-key>
			<column name="obj_id" />
		</primary-key>
		<attributes>
			<basic name="oid">
				<column name="obj_id" column-definition="decimal(10)"/>
				<generated-value generator="productTG" />
			</basic>
		</attributes>
	</entity>
	
	<entity class="com.kooobao.gsm.domain.entity.group.Group">
		<primary-key>
			<column name="obj_id" />
		</primary-key>
		<attributes>
			<basic name="oid">
				<column name="obj_id" column-definition="decimal(10)"/>
				<generated-value generator="groupTG" />
			</basic>
		</attributes>
	</entity>
</entity-mappings>
