<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings version="2.1"
	xmlns="http://www.eclipse.org/eclipselink/xsds/persistence/orm"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.eclipse.org/eclipselink/xsds/persistence/orm http://www.eclipse.org/eclipselink/xsds/eclipselink_orm_2_1.xsd">
	<table-generator name="bookTG" table="cws_sequence"
		pk-column-name="name" value-column-name="count" pk-column-value="cws_book" />
	<table-generator name="bookCategoryTG" table="cws_sequence"
		pk-column-name="name" value-column-name="count" pk-column-value="cws_book_category" />
	<table-generator name="articleTG" table="cws_sequence"
		pk-column-name="name" value-column-name="count" pk-column-value="cws_article" />

	<entity class="com.kooobao.cws.domain.book.Book">
		<primary-key>
			<column name="obj_id" />
		</primary-key>
		<attributes>
			<basic name="oid">
				<column name="obj_id" />
				<generated-value generator="bookTG" />
			</basic>
		</attributes>
	</entity>

	<entity class="com.kooobao.cws.domain.book.Category">
		<primary-key>
			<column name="obj_id" />
		</primary-key>
		<attributes>
			<basic name="oid">
				<column name="obj_id" />
				<generated-value generator="bookCategoryTG" />
			</basic>
		</attributes>
	</entity>

	<entity class="com.kooobao.cws.domain.article.Article">
		<primary-key>
			<column name="obj_id" />
		</primary-key>
		<attributes>
			<basic name="oid">
				<column name="obj_id" />
				<generated-value generator="articleTG" />
			</basic>
		</attributes>
	</entity>

</entity-mappings>
