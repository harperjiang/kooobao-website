<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE dwr PUBLIC "-//GetAhead Limited//DTD Direct Web Remoting 3.0//EN" "http://getahead.org/dwr//dwr30.dtd">
<dwr>
	<allow>
		<create creator="spring" javascript="cart_manager">
			<param name="beanName" value="cartManager" />
		</create>
		<create creator="spring" javascript="fav_manager">
			<param name="beanName" value="favManager" />
		</create>
		<create creator="spring" javascript="profile_service">
			<param name="beanName" value="profileService" />
			<include method="getVisitor" />
		</create>
		<convert match="com.kooobao.lm.profile.entity.Visitor"
			converter="bean" />
		<convert match="com.kooobao.lm.profile.entity.PersonalInfo"
			converter="bean" />
		<convert match="com.kooobao.lm.profile.entity.InstituteInfo"
			converter="bean" />
	</allow>
</dwr>  